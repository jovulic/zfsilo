run:
  go run -ldflags "-X 'github.com/jovulic/zfsilo/csi/internal/extvar.Version=local'" . start --config ./config.json 

test:
  go test ./...

test_short:
  go test -short ./...

build:
  go build -tags "json1"

generate:
  go generate ./...
  sed -i "s/-mod=mod //g" wire_gen.go

wire:
  wire

lint:
  golangci-lint run
