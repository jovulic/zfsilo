{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "simple",
      "versioning": "source-custom",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false
    }
  },
  "changelog-sections": [
    {
      "type": "ftr",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "doc",
      "section": "Documentation"
    },
    {
      "type": "tst",
      "section": "Tests"
    },
    {
      "type": "rvt",
      "section": "Reverts"
    },
    {
      "type": "sty",
      "section": "Styles"
    },
    {
      "type": "ref",
      "section": "Code Refactoring"
    },
    {
      "type": "bld",
      "section": "Build Changes"
    },
    {
      "type": "dpl",
      "section": "Deployment Changes"
    },
    {
      "type": "dev",
      "section": "Development Changes"
    },
    {
      "type": "rmv",
      "section": "Removals"
    },
    {
      "type": "add",
      "section": "General Changes"
    },
    {
      "type": "upd",
      "section": "General Changes"
    },
    {
      "type": "del",
      "section": "General Changes"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/jovulic/release-please/main/schemas/config.json"
}
